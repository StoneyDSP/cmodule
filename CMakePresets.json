    {
      "name": "toolchain",
      "displayName": "Configure preset using toolchain file",
      "toolchainFile": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
      "cacheVariables": { "CMAKE_TOOLCHAIN_FILE": { "type": "STRING", "value": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake" } },
      "inherits": [ "default" ],
      "hidden": true
    },
    {
      "name": "gcc",
      "displayName": "GCC 11.3.0 x86_64-linux-gnu",
      "description": "Using compilers: C = /usr/bin/gcc, CXX = /usr/bin/g++",
      "cacheVariables": {
        "CMAKE_C_COMPILER": {
          "type": "FILEPATH",
          "value": "/usr/bin/gcc"
        },
        "CMAKE_CXX_COMPILER": {
          "type": "FILEPATH",
          "value": "/usr/bin/g++"
        }
      },
      "inherits": [
        "default"
      ],
      "hidden": true
    },
    {
      "name": "clang",
      "displayName": "Clang 14.0.0 x86_64-pc-linux-gnu",
      "description": "Using compilers: C = /usr/bin/clang, CXX = /usr/bin/clang++",
      "cacheVariables": {
        "CMAKE_C_COMPILER": {
          "type": "FILEPATH",
          "value": "/usr/bin/clang"
        },
        "CMAKE_CXX_COMPILER": {
          "type": "FILEPATH",
          "value": "/usr/bin/clang++"
        }
      },
      "inherits": [
        "default"
      ],
      "hidden": true
    },
    {
      "name": "msvc",
      "cacheVariables": {
        "CMAKE_C_COMPILER": {
          "type": "FILEPATH",
          "value": "cl.exe"
        },
        "CMAKE_CXX_COMPILER": {
          "type": "FILEPATH",
          "value": "cl.exe"
        }
      },
      "inherits": [
        "default"
      ],
      "hidden": true
    },
    {
      "name": "x86",
      "displayName": "x86",
      "description": "Target 32-bit architecture.",
      "toolset": {
        "value": "x86",
        "strategy": "external"
      },
      "architecture": {
        "value": "x86",
        "strategy": "external"
      },
      "cacheVariables": {
        "NODE_ARCH": {
          "type": "STRING",
          "value": "x86"
        }
      },
